#: entrymgr.py:55 entrymgr.py:169
msgid "No entry name specified."
msgstr "Nessuna voce specificata."

#: entrymgr.py:114
msgid "Entry with filename '%s' exists."
msgstr "Esiste una voce con file '%s'."

#: entrymgr.py:139
msgid "Entry with filename '%s' does not exist."
msgstr "Non esiste una voce con file '%s'."

#: entrymgr.py:148
msgid "Manage journal entries."
msgstr "Gestisci le voci del diario."

#: entrymgr.py:151
msgid "Action to perform on entry, one of: create, remove."
msgstr "Azione da compiere sulla voce: create (crea) or remove (elimina)."

#: entrymgr.py:155
msgid "Name of entry to perform action on."
msgstr "Nome della voce sulla quale operare."

#: entrymgr.py:159
msgid "Date of entry to perform action on (Format: YYYY/MM/DD)."
msgstr "Data della voce sulla quale operare (Formato: AAAA/MM/GG)."

#: entrymgr.py:166
msgid "Invalid action, see --help."
msgstr "Azione non valida, consultare --help."

#: entrymgr.py:178
msgid "Invalid date format specified."
msgstr "Formato della data non valido."

#: entrymgr.py:182
msgid "Created entry '%s' for %s."
msgstr "Voce '%s' creata per %s."

#: entrymgr.py:186
msgid "Removed entry '%s' for %s."
msgstr "Voce '%s' eliminata per %s."
