#: entrymgr.py:55 entrymgr.py:169
msgid "No entry name specified."
msgstr "Aucune entrée de journal spécifié."

#: entrymgr.py:114
msgid "Entry with filename '%s' exists."
msgstr "Entrée de journal avec cette nom de fichier '%s' existe."

#: entrymgr.py:139
msgid "Entry with filename '%s' does not exist."
msgstr "Entrée de journal avec cette nom de fichier '%s' n'existe pas."

#: entrymgr.py:148
msgid "Manage journal entries."
msgstr "Gérer les écritures de journal."

#: entrymgr.py:151
msgid "Action to perform on entry, one of: create, remove."
msgstr "Action à effectuer sur l'entrée, l'un de: create (créer), remove (supprimer)."

#: entrymgr.py:155
msgid "Name of entry to perform action on."
msgstr "Nom d'entrée de journal à effectuer l'action sur."

#: entrymgr.py:159
msgid "Date of entry to perform action on (Format: YYYY/MM/DD)."
msgstr "Date d'entrée pour exécuter cette action sur (Format: AAAA/MM/JJ)."

#: entrymgr.py:166
msgid "Invalid action, see --help."
msgstr "Action invalide, consultez: --help."

#: entrymgr.py:178
msgid "Invalid date format specified."
msgstr "Format de date invalide spécifié."

#: entrymgr.py:182
msgid "Created entry '%s' for %s."
msgstr "Entrée '%s' pour %s créé."

#: entrymgr.py:186
msgid "Removed entry '%s' for %s."
msgstr "Entrée '%s' pour %s supprimé."
